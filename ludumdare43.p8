function update_gameover()
 if btn(🅾️) then
		mode = "menu"
	end
end

function victory_gameover()
 mode = "gameover"
 victory = true
end

function loss_gameover()
 mode = "gameover"
 victory = false
end

function draw_gameover()
 cls()
 if victory == true then
 	print("congratulations!", 34,28,7)
 	print("you rescued everyone!", 25,40,7)
 else
  print("game over",40,60,7)
 end
 print("press 🅾️ to restart", 30, 90,7)
end

