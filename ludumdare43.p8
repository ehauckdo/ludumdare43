function update_time()
	if current_time != get_time() then
  current_time = get_time()
  time_left = time_left - 1
  if time_left == 0 then
  	if total_rescued >= total_victims/2 then
  	 victory_gameover()
  	else
  	 loss_gameover()
  	end
  end
 end
end

function update_player()
	p = player
	if p.mov > 0 then
	 p.step += 1
  if p.step > 10 then
   p.step = 0
   p.f = (p.f%4) +1
  end
  speed = 1
  if p.rescuing != nil then
   if p.rescuing.t == 1 then
    speed = 0.8
   elseif p.rescuing.t == 2 then
    speed = 0.5
   elseif p.rescuing.t == 3 then
    speed = 1
   end
  end

	  if p.dir == "‚¨ÜÔ∏è" then p.offsety -= speed end
	  if p.dir == "‚¨áÔ∏è" then p.offsety += speed end
	  if p.dir == "‚¨ÖÔ∏è" then p.offsetx -= speed end
	  if p.dir == "‚û°Ô∏è" then p.offsetx += speed end

	  if abs(p.offsetx) >= 12 or abs(p.offsety) >= 12 then
	   if p.dir == "‚¨ÜÔ∏è" then p.y -=1 end
	   if p.dir == "‚¨áÔ∏è" then p.y +=1 end
	   if p.dir == "‚¨ÖÔ∏è" then p.x -=1 end
	   if p.dir == "‚û°Ô∏è" then p.x +=1 end
	   p.offsety = 0
	   p.offsetx = 0
	   p.mov = 0
	  end
	else

	 floor = floors[cur_floor]
	 if btn(‚¨ÖÔ∏è) and p.x > 1 and inspect_id(floor,p.x-1,p.y) != "fire" then p.dir = "‚¨ÖÔ∏è" p.mov = 12
	 elseif btn(‚û°Ô∏è) and p.x < 10 and inspect_id(floor,p.x+1,p.y) != "fire" then	p.dir = "‚û°Ô∏è" p.mov = 12
	 elseif btn(‚¨ÜÔ∏è) and p.y > 1 and inspect_id(floor,p.x,p.y-1) != "fire"  then	p.dir = "‚¨ÜÔ∏è" p.mov = 12
	 elseif btn(‚¨áÔ∏è) and p.y < 9 and inspect_id(floor,p.x,p.y+1) != "fire" then	p.dir = "‚¨áÔ∏è" p.mov = 12 end

	end
end

function update_fire()
 for i=1,10 do
  for j=1,9 do
   floor = floors[cur_floor]
   if floor[i][j].item != nil and floor[i][j].item.id == "fire" then
    f = floor[i][j].item
    f.step += 1
    if f.step > 20 then
     f.step = 0
     f.f = (f.f%3) +1
    end
   end
  end
 end
end

function check_coll()
	floor = floors[cur_floor]
	if floor[player.x][player.y].item != nil then
	 item = floor[player.x][player.y].item

	 if item.id == "stair" then
	  if item.dir == "‚¨ÜÔ∏è" then
	   cur_floor += 1
	   player.y = player.y-1
	  elseif item.dir == "‚¨áÔ∏è" then
	   cur_floor -= 1
	   player.y = player.y+1
	  end
	 end

	 if item.id == "person" then
	  if btn(‚ùé) then
	   player.rescuing = item
	   floor[player.x][player.y].item = nil
	  end
	 end

	 if item.id == "safe" then
	   if player.rescuing != nil then
	    player.rescuing = nil
	    total_rescued += 1
	    if total_rescued == total_victims then
	     victory_gameover()
	    end
	  end
	 end

	end
end

