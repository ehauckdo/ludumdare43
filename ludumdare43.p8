function sprite(x,y,h,w,s)
 local sprite = {}
 sprite.x = x
 sprite.y = y
 sprite.h = h
 sprite.w = w
 if s != nil then
  sprite.s = s
 end
 return sprite
end

function get_player_sprite(player)
 player = p
 spr_id = p.ss[p.dir][p.f]
 return get_sprite(spr_id)
end

function get_sprite(id)

 if id < 8 then
  n = id*2
 elseif id < 16 then
  n = 32+((id-8)*2)
 elseif id < 24 then
  n = 64+((id-16)*2)
 elseif id < 32 then
  n = 96+((id-24)*2)
 elseif id < 40 then
  n = 128+((id-32)*2)
 elseif id < 48 then
  n = 156+((id-40)*2)
 elseif id < 70 then
  n = 188+((id-48)*2)
 end

 return n
end

function get_grid_pos(x,y)
 coord = {}
 coord["x"] = 4+((x-1)*12)
 coord["y"] = 16+((y-1)*12)
 return coord
end

function create_matrix(x,y)
 matrix = {}
 for i=1,x do
  column = {}
  for j=1,y do
   field = {}
   field.tile = 66
   field.item = nil
   add(column,field)
  end
  add(matrix,column)
 end
 return matrix
end

function find_in_matrix(matrix,id)
 for i=1,10 do
  for j=1,10 do
   if matrix[i][j].item != nil then // == id then
    return matrix[i][j].item
   end

  end
 end
end
