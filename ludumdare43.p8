function calculate_min_rescued()
	// simple bubble sort to order
	// people by distance from door
	lv = list_victims
	for i=1,#lv-1 do
	 for j=i+1,#lv do
	  if lv[i].dist > lv[j].dist then
	   temp = lv[i]
	   lv[i] = lv[j]
	   lv[j] = temp
	  end
	 end
	end
	//cls()
	//for i=1,#lv do
	// print(list_victims[i].dist)
	//end
	//stop()
	minimum = 0
	local t_left = time_left
	for p in all(lv) do
	 t_left -= flr(p.dist/2) + 2
	 if t_left >= 0 then
	  minimum += 1
	 else break
	 end
	end
end

