function get_sprite(id)

 if id < 8 then
  n = id*2
 elseif id < 16 then
  n = 32+((id-8)*2)
 elseif id < 24 then
  n = 64+((id-16)*2)
 elseif id < 32 then
  n = 96+((id-24)*2)
 elseif id < 40 then
  n = 128+((id-32)*2)
 elseif id < 48 then
  n = 160+((id-40)*2)
 elseif id < 56 then
  n = 192+((id-48)*2)
 elseif id < 64 then
  n = 224+((id-56)*2)
 elseif id < 72 then
  n = 256+((id-64)*2)
 end

 return n
end

function get_grid_pos(x,y)
