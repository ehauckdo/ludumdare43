 coord["x"] = 4+((x-1)*12)
 coord["y"] = 16+((y-1)*12)
 return coord
end

function create_matrix(x,y)
 matrix = {}
 for i=1,x do
  column = {}
  for j=1,y do
   field = {}
   field.tile = 66
   field.item = nil
   if rnd() > 0.90 then
    field.item = load_fire(i,j)
   end
   add(column,field)
  end
  add(matrix,column)
 end
 return matrix
end

