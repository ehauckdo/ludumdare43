function start_game()
 mode = "game"
 total_rescued = 0
 load_time()
 load_player()
 load_floors()
end

function load_time()
	current_time = flr(time())
	time_left = 120
end

function load_player()
 player = {}
 player.x = 2
 player.y = 9
 player.h = 1.5
 player.w = 1.5
 player.ss = {}
 player.ss["⬇️"] = {0,1,0,2}
 player.ss["⬆️"] = {3,4,3,5}
 player.ss["➡️"] = {9,10,9,11}
 player.ss["⬅️"] = {12,13,12,14}
 player.dir = "⬇️"
 player.f = 1
 player.mov = 0
 player.step = 1
 player.col = false
end

function load_floors()
 cur_floor = 1
 total_people = 0

 floors = {}
 top_floor = flr(rnd(4))+2
 for i=1,top_floor do
  f = create_matrix(10,9)

  // creating 1-3 rand people
  for j=1, flr(rnd(3))+1 do
   person = load_person()
   if f[person.x][person.y].item == nil then
   	f[person.x][person.y].item=person
   	total_people += 1
   end
  end

  if i > 1 then
   stair_d = load_stair_down(stair)
   while f[stair_d.x][stair_d.y].item != nil do
    stair_d = load_stair_down(stair)
   end
   f[stair_d.x][stair_d.y].item=stair_d

  end

  if i < top_floor then
   stair = load_stair_up()
   f[stair.x][stair.y].item=stair
  end
  add(floors,f)
 end

 floors[1][2][9].item = {}
 floors[1][2][9].item.id = "safe"

end

function load_person()
 local sprites = {192,194,1964,198,200}
 person = {}
 person.x = flr(rnd(10))+1
 person.y = flr(rnd(9))+1
 person.h = 1.5
 person.w = 1.5
 person.s = sprites[flr(rnd(5))+1]
 person.drag = false
 person.rescued = false
 person.id = "person"
	return person
end

function load_stair_up()
 stair = {}
 stair.x = flr(rnd(10))+1
 stair.y = flr(rnd(8))+2
 stair.dir = "⬆️"
 stair.id = "stair"
 return stair
end

function load_stair_down(s)
 stair = {}
 stair.x = s.x
 stair.y = s.y
 stair.dir = "⬇️"
 stair.id = "stair"
 return stair
end

function update_game()
 update_time()
 update_player()
 check_coll()
 if btn(🅾️) then
  if pressed != true then
   local p = player
   p.f = p.f%#p.ss[p.dir] + 1
   pressed=true
  end
 else
  pressed = false
 end
end

function update_time()
	if current_time != flr(time()) then
  current_time = flr(time())
  time_left = time_left - 1
  if time_left == 0 then
  	loss_gameover()
  end
 end
end

function update_player()
	p = player
	if p.mov > 0 then
	 p.mov -= 1
	else
	 if btn(⬅️) and p.x > 1 then p.x -= 1 p.dir = "⬅️" p.mov = 10
	 elseif btn(➡️) and p.x < 10 then	p.x += 1 p.dir = "➡️" p.mov = 10
	 elseif btn(⬆️) and p.y > 1 then	p.y -= 1 p.dir = "⬆️" p.mov = 10
	 elseif btn(⬇️) and p.y < 9 then	p.y += 1 p.dir = "⬇️" p.mov = 10 end
	end
end

function check_coll()
	floor = floors[cur_floor]
	if floor[player.x][player.y].item != nil then
	 item = floor[player.x][player.y].item

	 if item.id == "stair" then
	  if item.dir == "⬆️" then
	   cur_floor += 1
	   player.y = player.y-1
	  elseif item.dir == "⬇️" then
	   cur_floor -= 1
	   player.y = player.y+1
	  end
	 end

	 if item.id == "person" then
	  if btn(❎) then
	   player.rescuing = item
	   floor[player.x][player.y].item = nil
	  end
	 end

	 if item.id == "safe" then
	   if player.rescuing != nil then
	    player.rescuing = nil
	    total_rescued += 1
	    if total_rescued == total_people then
	     victory_gameover()
	    end
	  end
	 end

	end
end

function draw_game()
	cls(0)
	draw_score()
	draw_floor()
	draw_walls()
	draw_player()
end

function draw_score()
 //print("time until collapse: 00:"..time_left,0,0,7)
 print(total_people..", "..total_rescued,0,0,7)
end

function draw_floor()
 floor = floors[cur_floor]
 for i=1,#floor do
	 for j=1,#floor[i] do
	  draw_tile(floor[i][j].tile,i,j)
	 end
 end

	for i=1,#floor do
	 for j=1,#floor[i] do

   if floor[i][j].item != nil then
    if floor[i][j].item.id == "person" then
     draw_person(floor[i][j].item)
    elseif floor[i][j].item.id == "stair" then
     draw_stair(floor[i][j].item)
    end
   end
	 end
	end
end


function draw_tile(s,x,y)
 coord = get_grid_pos(x,y)
 spr(s,coord.x,coord.y,1.5,1.5)
end

function draw_player()
 p = player
 sprite = get_player_sprite()
 coord = get_grid_pos(p.x,p.y)
 spr(sprite, coord.x, coord.y, p.w, p.h)
 if player.rescuing != nil then
  sprite = player.rescuing.s
  coord = get_grid_pos(player.x,player.y)
  spr(sprite,coord.x+4,coord.y+4,1.5,1.5)
 end
end

function draw_person(p)
 sprite = p.s
 coord = get_grid_pos(p.x,p.y)
 spr(224,coord.x,coord.y, 1.5, 1.5)
 spr(sprite, coord.x, coord.y, p.w, p.h)
end

function draw_stair(s)
	if s.dir == "⬆️" then
	 sprite = 76
	elseif s.dir == "⬇️" then
	 sprite = 78
	end
	coord = get_grid_pos(s.x,s.y)
	spr(sprite,coord.x,coord.y, 2,2)
end

function draw_walls()
 rectfill(0,8,127,15,1)
 rectfill(0,12,8,127,1)
 rectfill(124,12,127,127,1)
	rectfill(0,124,127,127,1)
	if cur_floor == 1 then spr(98,20,124,1.5,1.5) end
end
